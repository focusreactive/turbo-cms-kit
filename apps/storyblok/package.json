{
  "name": "storyblok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=4050 --experimental-https --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sb-login": "pnpm storyblok login",
    "pull-schemas": "pnpm storyblok pull-components --space 293915 --file-name production --path src/generated/",
    "push-schemas": "pnpm storyblok push-components src/generated/components.production.json --presets-source src/generated/presets.production.json --space",
    "generate-types": "pnpm pull-schemas && storyblok generate-typescript-typedefs --sourceFilePaths ./src/generated/components.production.json --destinationFilePath src/generated/extracted-types.ts"
  },
  "dependencies": {
    "@shared/ui": "workspace:*",
    "@storyblok/react": "^4.0.0",
    "next": "15.0.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "storyblok-rich-text-react-renderer": "^2.9.2"
  },
  "devDependencies": {
    "@shared/eslint-config": "workspace:*",
    "@shared/tailwind-config": "workspace:*",
    "@shared/ts-config": "workspace:*",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "19.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "storyblok": "^3.34.0",
    "tailwindcss": "^3.4.3",
    "typescript": "5.5.3"
  }
}
